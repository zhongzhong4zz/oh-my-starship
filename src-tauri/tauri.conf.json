{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "oh-my-starship",
  "version": "0.1.0",
  "identifier": "com.starship.config-manager",
  "build": {
    "beforeDevCommand": "pnpm ui:dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm ui:build",
    "frontendDist": "../dist"
  },
  "app": {
    "macOSPrivateApi": true,
    "windows": [
      {
        "title": "oh-my-starship",
        "width": 900,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
